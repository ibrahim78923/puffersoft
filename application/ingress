apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: application-ingress
  namespace: app

spec:
  ingressClassName: treafik-traefik
  rules:
   - host: www.dreggystore.com
     http:
        paths:
          - path: /application
            pathType: Prefix
            backend:
              service:
                name:  web-svc
                port:
                  number: 80
