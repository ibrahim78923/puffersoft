apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: application-ingress
  namespace: app
  annotations:
    traefik.backend.loadbalancer.sticky: "true"
spec:
  ingressClassName: treafik-traefik
  rules:
  - host: www.dreggystore.com
    http:
        paths:
          - path: /app
            pathType: Exact
            backend:
              service:
                name:  web-svc
                port:
                  number: 80
